package com.java.Daimond;

public class DaimontPattern {

	public static void main(String[] args) {
//		System.out.println("*");
		
		String stars = " ";
		for(int i = 0; i < 3; i++) {
			for(int j = i +1; j > 3 ; j++) {
				System.out.println("*");
			}
//			System.out.println("*");
		}
		
		

	}

}
