package com.java.Daimond;

public class DaimontPattern {

	public static void main(String[] args) {
//		System.out.println("*");
		
		public String createDimond(String star) {
			return "";
		}
		String stars = " ";
		for(int i = 0; i < 3; i++) {
			for(int j = 0; j < 2 ; j++) {
				stars = stars + "*";
			}
		return stars;
//			System.out.println("*");
		}
		

	}

}
